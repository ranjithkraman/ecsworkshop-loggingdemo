{
  "executionRoleArn": "{{EXECUTIONROLEARN}}",
  "containerDefinitions": [
    {
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "{{LOGGROUPNAME}}",
          "awslogs-region": "{{REGION}}",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "portMappings": [
        {
          "protocol": "tcp",
          "containerPort": 8000
        }
      ],
      "mountPoints": [
        {
          "readOnly": false,
          "containerPath": "/mnt/fs",
          "sourceVolume": "efs-vol"
        }
      ],
      "environment": [
        {
          "name": "CLOUDCMD_ROOT",
          "value": "/mnt/fs"
        }
      ],
      "image": "coderaiser/cloudcmd:latest",
      "essential": true,
      "name": "cloudcmd-rw"
    }
  ],
  "memory": "512",
  "cpu": "256",
  "taskRoleArn": "{{TASKROLEARN}}",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "family": "cloudcmd-rw",
  "networkMode": "awsvpc",
  "volumes": [
    {
      "efsVolumeConfiguration": {
        "fileSystemId": "{{FSID}}",
        "rootDirectory": "/"
      },
      "name": "efs-vol"
    }
  ]
}
